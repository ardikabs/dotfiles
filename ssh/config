Host 10.*
    User ec2-user
    Port 22
    PreferredAuthentications password

Host 10.10.*
    User ec2-user
    Port 22

Host 10.10.10.*
    User root
    Port 4222
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

Host jumper-sample
    HostName 172.18.1.10
    User jumper-user
    Port 4222

Host protected-host
    HostName 172.18.2.10
    User root
    Port 4222
    ProxyCommand ssh -q -W %h:%p jumper-sample

Host 172.18.2.*
    User root
    Port 4222
    ProxyCommand ssh -q -W %h:%p jumper-sample
    #ProxyJump jumper-sample