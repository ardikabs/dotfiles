Match host * exec "gpg-connect-agent UPDATESTARTUPTTY /bye"
    ForwardAgent yes
    AddKeysToAgent yes

Host *
    Protocol 2
    LogLevel INFO
    Compression yes
    ServerAliveInterval 60s
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host gitlab.com github.com source.golabs.io
    PreferredAuthentications publickey

Host 10.*
    User ec2-user
    Port 22
    PreferredAuthentications publickey

Host 10.10.*
    User ec2-user
    Port 22
    PreferredAuthentications publickey

Host 10.10.10.*
    User root
    Port 4222
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

Host jumper-sample
    HostName 172.18.1.10
    User root
    Port 4222
    ProxyCommand ssh -q -W %h:%p kadabra@kadabra-jumper
    PreferredAuthentications publickey